{
  "name": "paintvec",
  "version": "0.7.1",
  "description": "JavaScript 2D vector math library",
  "keywords": [
    "geometry",
    "vector",
    "rectangle",
    "affine"
  ],
  "homepage": "https://github.com/seanchas116/paintvec#readme",
  "bugs": {
    "url": "https://github.com/seanchas116/paintvec/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanchas116/paintvec.git"
  },
  "license": "MIT",
  "author": "",
  "type": "module",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "prettier --write *.ts && tsc",
    "docs:dev": "vitepress dev docs",
    "docs:build": "npm run docs:generate && vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:generate": "typedoc",
    "test": "vitest run -r ."
  },
  "devDependencies": {
    "@types/node": "^24.0.13",
    "docsify-cli": "^4.4.4",
    "prettier": "^3.6.2",
    "typedoc": "^0.28.7",
    "typedoc-plugin-markdown": "^4.7.0",
    "typedoc-theme-clean": "github:seanchas116/typedoc-theme-clean",
    "typescript": "^5.8.3",
    "vitepress": "^1.6.3",
    "vitest": "^3.2.4"
  }
}
